[package]
name = "rustlibd"
version = "0.1.0"
edition = "2021"

[lib]
name = "rustlibd"
path = "@srcdir@/rustlib_lib.rs"
crate-type = ["staticlib"]

[dependencies]
tracing = "0.1.40"
tracing-subscriber = { version = "0.3.18", features = ["env-filter"]}

[build-dependencies]
bindgen = "0.70.1"
cc = "1.1.18"
